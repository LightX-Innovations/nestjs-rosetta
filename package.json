{
    "name": "@recursyve/nestjs-rosetta",
    "version": "1.0.0",
    "scripts": {
        "build": "gulp build --dist node_modules/@recursyve",
        "prebuild:prod": "npm run clean",
        "build:prod": "gulp build",
        "test": "jest",
        "clean": "gulp clean:bundle",
        "prerelease": "gulp copy-misc && gulp build --dist node_modules/@recursyve",
        "publish": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --force-publish --access public --exact -m \"Publish %s release\"",
        "publish:beta": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --npm-tag=beta --access public -m \"Publish %s release\""
    },
    "peerDependencies": {
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "rxjs": "^7.5.5",
        "accept-language-parser": "^1.5.0",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@nestjs/common": "^9.2.1",
        "@nestjs/core": "^9.2.1",
        "@nestjs/platform-express": "^9.2.1",
        "@nestjs/sequelize": "^9.0.0",
        "@types/accept-language-parser": "^1.5.3",
        "@types/body-parser": "^1.19.2",
        "@types/connect": "^3.4.35",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.2.5",
        "@types/ms": "^0.7.31",
        "@types/node": "^18.11.18",
        "@types/validator": "^13.7.10",
        "accept-language-parser": "^1.5.0",
        "delete-empty": "^3.0.0",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^29.3.1",
        "lerna": "2.11.0",
        "mongoose": "^6.8.2",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0",
        "sequelize": "^6.28.0",
        "sequelize-typescript": "^2.1.5",
        "sqlite3": "^5.1.4",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "typescript": "^4.9.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "packages",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "collectCoverage": true
    },
    "dependencies": {
        "@nestjs/mongoose": "^9.2.1"
    }
}
