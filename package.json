{
    "name": "@recursyve/nestjs-rosetta",
    "version": "1.0.0",
    "scripts": {
        "build": "gulp build --dist node_modules/@recursyve",
        "prebuild:prod": "npm run clean",
        "build:prod": "gulp build",
        "test": "jest",
        "clean": "gulp clean:bundle",
        "prerelease": "gulp copy-misc && gulp build --dist node_modules/@recursyve",
        "publish": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --force-publish --access public --exact -m \"Publish %s release\"",
        "publish:beta": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --npm-tag=beta --access public -m \"Publish %s release\""
    },
    "peerDependencies": {
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "rxjs": "^7.5.5",
        "accept-language-parser": "^1.5.0",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@nestjs/common": "^8.0.5",
        "@nestjs/core": "^8.0.5",
        "@nestjs/platform-express": "^8.0.5",
        "@nestjs/sequelize": "^8.0.0",
        "@types/accept-language-parser": "^1.5.3",
        "@types/body-parser": "^1.19.2",
        "@types/connect": "^3.4.35",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/ms": "^0.7.31",
        "@types/node": "^14.17.7",
        "@types/sequelize": "^4.28.11",
        "@types/validator": "^13.7.2",
        "accept-language-parser": "^1.5.0",
        "delete-empty": "^3.0.0",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^27.5.1",
        "lerna": "2.11.0",
        "mongoose": "^6.6.5",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.5",
        "sequelize": "^6.19.0",
        "sequelize-typescript": "^2.1.0",
        "sqlite3": "^5.0.4",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "tslint": "^6.1.3",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "packages",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "collectCoverage": true
    }
}
